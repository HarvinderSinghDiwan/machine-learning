    "# libraries\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    " \n",
    "# An histogram 2D\n",
    "x = np.random.normal(size=50000)\n",
    "y = x * 3 + np.random.normal(size=50000)\n",
    "plt.hist2d(x, y, bins=(50, 50), cmap=plt.cm.Reds)\n",
    " \n",
    "# Add a basic title\n",
    "plt.title(\"A 2D histogram\")\n",
    "plt.grid(color='blue')"
   ]
